{
  "name": "nutanix_deployment_manager",
  "version": "0.5.0",
  "description": "Datacenter Management Suite for Nutanix based infrastructure",
  "main": "index.js",
  "scripts": {
    "db:fresh": "knex --knexfile ./src/database/knexfile.js migrate:rollback --all && npm run db:migrate",
    "db:migrate": "knex --knexfile ./src/database/knexfile.js migrate:latest",
    "db:down": "knex --knexfile ./src/database/knexfile.js migrate:down",
    "db:up": "knex --knexfile ./src/database/knexfile.js migrate:up",
    "db:newMigration": "knex --knexfile ./src/database/knexfile.js migrate:make",
    "db:rollback": "knex --knexfile ./src/database/knexfile.js migrate:rollback",
    "db:newSeed": "knex --knexfile ./src/database/knexfile.js seed:make",
    "db:seed": "knex --knexfile ./src/database/knexfile.js seed:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite --config ./src/ui/frontend/vite.config.js",
    "build": "vite build --config ./src/ui/frontend/vite.config.js",
    "buildDev": "vite build --config ./src/ui/frontend/vite.config.js --mode 'development'",
    "preview": "vite preview --config ./src/ui/frontend/vite.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutanix-outcome-engineering/nutanix_deployment_manager"
  },
  "author": "Outcome_Engineering@nutanix.com",
  "license": "Apache 2.0",
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^30.1.0",
    "@ag-grid-community/core": "^30.1.0",
    "@ag-grid-community/styles": "^30.1.0",
    "@ag-grid-community/vue3": "^30.1.0",
    "@headlessui/tailwindcss": "^0.2.0",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^2.0.13",
    "@tus/file-store": "^1.3.0",
    "@tus/server": "^1.4.0",
    "axios": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.10.0",
    "busboy": "^1.6.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.1",
    "express-session": "^1.17.3",
    "flat": "^5.0.2",
    "fuse.js": "^6.6.2",
    "graphology": "^0.25.1",
    "graphology-dag": "^0.4.0",
    "json-schema-traverse": "^1.0.0",
    "jsondiffpatch": "^0.4.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.2",
    "netmask": "^2.0.2",
    "nutanix_foundation": "git+https://github.com/nutanix-outcome-engineering/nutanix_foundation.git",
    "nutanix_prism": "git+https://github.com/nutanix-outcome-engineering/nutanix_prism.git",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "ssh2": "^1.14.0",
    "stack-trace": "^0.0.10",
    "tus-js-client": "^3.1.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "vanilla-text-mask": "^5.1.1",
    "vite": "^4.0.4",
    "vue-tippy": "^6.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "src/ui/frontend/*",
      "node_modules/*",
      "files/*"
    ],
    "watch": [
      "*.*",
      ".env"
    ]
  }
}
