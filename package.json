{
  "name": "rx-lite",
  "version": "1.0.0",
  "description": "Reservation System and Datacenter Management Suite for Nutanix based infrastructure",
  "main": "index.js",
  "scripts": {
    "db:fresh": "knex --knexfile ./src/database/knexfile.js migrate:rollback --all && npm run db:migrate",
    "db:migrate": "knex --knexfile ./src/database/knexfile.js migrate:latest",
    "db:down": "knex --knexfile ./src/database/knexfile.js migrate:down",
    "db:up": "knex --knexfile ./src/database/knexfile.js migrate:up",
    "db:newMigration": "knex --knexfile ./src/database/knexfile.js migrate:make",
    "db:rollback": "knex --knexfile ./src/database/knexfile.js migrate:rollback",
    "db:newSeed": "knex --knexfile ./src/database/knexfile.js seed:make",
    "db:seed": "knex --knexfile ./src/database/knexfile.js seed:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite --config ./src/ui/frontend/vite.config.js",
    "build": "vite build --config ./src/ui/frontend/vite.config.js",
    "preview": "vite preview --config ./src/ui/frontend/vite.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rtp.nutanix.com:corp-se-team/rx-lite.git"
  },
  "author": "Outcome_Engineering@nutanix.com",
  "license": "MIT",
  "dependencies": {
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^2.0.13",
    "axios": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.5.10",
    "busboy": "^1.6.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.1",
    "express-session": "^1.17.3",
    "flat": "^5.0.2",
    "graphology": "^0.25.1",
    "graphology-dag": "^0.2.0",
    "jsondiffpatch": "^0.4.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "netmask": "^2.0.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "vanilla-text-mask": "^5.1.1",
    "vite": "^4.0.4",
    "vue-tippy": "^6.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "src/ui/frontend/*",
      "node_modules/*"
    ],
    "watch": [
      "*.*",
      ".env"
    ]
  }
}
